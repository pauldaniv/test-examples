worker_processes  1;

events {
  worker_connections  1024;
}

http {
  include       mime.types;
  default_type  application/octet-stream;

  sendfile        on;

  server {

    listen 80 default_server;

    location / {
      proxy_pass http://fe/;
    }

    location /be {
      proxy_pass http://store:8787/;
    }

    error_page   500 502 503 504  /50x.html;
    location = /50x.html {
      root   html;
    }
  }
}
